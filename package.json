    "gatsby": "^4.25.7",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-catch-links": "^5.10.0",
    "gatsby-plugin-google-analytics": "^5.10.0",
    "gatsby-plugin-netlify": "^5.10.0",
    "gatsby-plugin-netlify-cache": "^5.10.0",
    "gatsby-plugin-nprogress": "^5.10.0",
    "gatsby-plugin-offline": "^5.10.0",
    "gatsby-plugin-react-helmet": "^5.10.0",
    "gatsby-plugin-robots-txt": "^5.10.0",
    "gatsby-plugin-sharp": "^5.10.0",
    "gatsby-plugin-sitemap": "^5.10.0",
    "gatsby-plugin-styled-components": "^5.10.0",
    "gatsby-plugin-typescript": "^5.10.0",
    "gatsby-remark-copy-linked-files": "^5.10.0",
    "gatsby-remark-images": "^5.10.0",
    "gatsby-remark-smartypants": "^5.10.0",
    "gatsby-source-filesystem": "^5.10.0",
    "gatsby-transformer-remark": "^5.10.0",
    "gatsby-transformer-sharp": "^5.10.0",
    "gatsby-transformer-sqip": "^5.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.3.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "start": "gatsby develop",
    "build": "gatsby build",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}' -p tsconfig.json && tsc",
    "type-check": "tsc --project tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run type-check",
      "pre-write": "lint-staged && npm run type-check"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "git add",
      "tslint --fix -p tsconfig.json",
      "git add"
    ],
    "*.{graphql,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-styled-components": "^1.10.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "tslint": "^6.1.3",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.4.3"
  },
  "browserslist": "> 1% in US"
}
